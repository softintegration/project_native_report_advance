<?xml version="1.0"?>
<openerp>
    <data>

        <record id="project_native_report_form" model="ir.ui.view">
            <field name="name">project.native.report.form</field>
            <field name="model">project.native.report</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Export xml">

                    <sheet>
                        <group>
                            <group>
                                <field name="group_id"/>
                                <field name="name"/>
                            </group>
                        </group>
                    </sheet>

                    <footer>
                        <button name="print_pdf" string="Gantt to PDF" type="object" class="btn-primary"/>
                    </footer>

                </form>
            </field>
        </record>

    </data>
</openerp>
